{
  "name": "nextjs-theme-ui",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "find:unused": "next-unused",
    "prettier": "prettier"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages",
      "sections"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  },
  "dependencies": {
    "@builder.io/react": "^1.1.44",
    "@builder.io/utils": "^1.0.3",
    "@builder.io/widgets": "^1.2.19",
    "@theme-ui/components": "^0.6.2",
    "@theme-ui/css": "^0.12.0",
    "@theme-ui/match-media": "^0.7.2",
    "@theme-ui/preset-base": "^0.6.0",
    "@theme-ui/presets": "^0.6.2",
    "cheerio": "^1.0.0-rc.6",
    "jest": "^26.6.3",
    "js-cookie": "^2.2.1",
    "next": "^10.1.2",
    "next-seo": "^4.11.0",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-spring-modal": "^2.0.7",
    "sanitize.css": "^13.0.0",
    "theme-ui": "^0.6.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.0.1",
    "@types/js-cookie": "^2.2.6",
    "@types/react": "^16.9.49",
    "next-unused": "^0.0.3",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  },
  "resolutions": {
    "webpack": "^5.0.0-beta.30"
  },
  "license": "MIT"
}
